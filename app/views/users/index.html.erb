<div class="container">
  <div class="row">

    <!--部分テンプレ-->

    <div class="col-md-8 offset-md-1">
      <h1>Users</h1>
      <table>
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
            <th></th>
          </tr>
        </thead>
      <% @users.each do |user| %>
        <tbody>
          <tr>
            <td><%= image_tag user.get_profile_image(80,80) %></td>
            <td><%= user.name %></td>
            <td><%= link_to "Show", user_path(user.id) %></td>
          </tr>
        </tbody>
      <% end %>
      </table>
    </div>

  </div>
</div>